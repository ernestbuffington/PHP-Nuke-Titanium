<table border="0" cellpadding="4" cellspacing="1" width="100%" class="forumline">
    <!-- BEGIN popup_switch -->
    <tr> 
      <th colspan="3" class="thHead">{L_PRIVATE_MESSAGES}</th>
    </tr>
    <!-- END popup_switch -->
    <tr> 
      <td class="row2">{L_FROM}:</td>
      <td width="100%" class="row2" colspan="2">{FROM}{FROM_IP}</td>
    </tr>
    <tr> 
      <td class="row2">{L_TO}:</td>
      <td width="100%" class="row2" colspan="2">{TO}</td>
    </tr>
    <tr> 
      <td class="row2">{L_SENT_DATE}:</td>
      <td width="100%" class="row2" colspan="2">{DATE}</td>
    </tr>
    <tr> 
      <td class="row2">{L_SUBJECT}:</td>
      <td width="100%" class="row2">{SUBJECT}</td>
      <td nowrap="nowrap" class="row2" align="right"> &nbsp; </td>
    </tr>
    <tr> 
      <td valign="top" colspan="3" class="row1"><span class="postbody">{MESSAGE}</span></td>
    </tr>
</table><br />
